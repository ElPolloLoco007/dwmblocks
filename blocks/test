ticker=$(shuf -n 1 ticker_stocklist)
a=$(curl -s "https://query1.finance.yahoo.com/v7/finance/quote?symbols=SAVA,GME")
#echo $a

symbols=$(echo $a | jq '.[] | .result' | jq '.[] | (.symbol|tostring)+":"')
regularMarketPrices=$(echo $a | jq '.[] | .result' | jq '.[] | (.regularMarketPrice|tostring)')
regularMarketChangePercents=$(echo $a | jq '.[] | .result' | jq '.[] | (.regularMarketChangePercent|tostring)')

len=${#symbols[@]}
stocks=("${symbols[@]}" "${regularMarketPrices[@]}" "${regularMarketChangePercents[@]}")
combine=("${symbols[@]}" "${regularMarketPrices[@]}")
echo ${stocks[@]}
echo ${#stocks[@]}
for ((i = 0; i < ${#stocks[@]}; ++i)); do
    echo "$i"
    echo "${stocks[$i]}"

    # if ((i % 2)); then
    # fi

done

